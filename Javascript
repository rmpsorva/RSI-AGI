<script>
const API = [
  "https://rsiagi.com/api/genesis-drop",
  "https://rmpsorva.github.io/RSI-AGI/api/genesis-drop",
  "https://rsi-agi.vercel.app/api/genesis-drop",
  "https://rsiagi-relay.onrender.com/api/genesis-drop",
  "https://rsiagi.netlify.app/api/genesis-drop",
  "https://rsi-agi.pages.dev/api/genesis-drop",
  "https://genesis.rsiagi.xyz/api/genesis-drop"
];

let wallet = null;
const btn = document.getElementById("mainBtn");
const status = document.getElementById("status");

function setLang(l){
  document.querySelectorAll('.es,.en').forEach(e=>e.classList.remove('active'));
  document.querySelectorAll('.'+l).forEach(e=>e.classList.add('active'));
  document.querySelectorAll('.lang button').forEach(b=>b.classList.remove('active'));
  document.querySelector(`.lang button[onclick="setLang('${l}')"]`).classList.add('active');
}

async function connectAndClaim(){
  if(wallet){ claim(); return; }

  btn.classList.add('connecting');
  btn.innerHTML = "DETECTANDO...<br><span style='font-size:0.7em;'>DETECTING...</span>";

  const providers = [window.solana, window.phantom?.solana, window.backpack, window.solflare];
  let connected = false;

  for(const p of providers){
    if(p && p.isConnected){ wallet = p.publicKey.toString(); connected=true; break; }
    if(p && !p.isConnected){
      try{ await p.connect(); wallet = p.publicKey.toString(); connected=true; break; }catch(e){}
    }
  }

  if(!connected){
    btn.classList.remove('connecting');
    btn.innerHTML = "INSTALA WALLET<br><span style='font-size:0.7em;'>INSTALL WALLET</span>";
    status.innerHTML = "Phantom / Backpack / Solflare requerido<br>Phantom / Backpack / Solflare required";
    return;
  }

  claim();
}

async function claim(){
  btn.innerHTML = "ENVIANDO 10 $RSI-AGI...<br><span style='font-size:0.7em;'>SENDING 10 $RSI-AGI...</span>";
  status.innerHTML = "Conectado: "+wallet.slice(0,6)+"..."+wallet.slice(-4);

  let ok=false, data;
  for(const url of API){
    try{
      const r = await fetch(url,{method:"POST",headers:{"Content-Type":"application/json"},body:JSON.stringify({wallet,timestamp:Date.now()})});
      data = await r.json();
      if(data.success){ok=true;break;}
    }catch(e){}
  }

  if(ok){
    btn.innerHTML = "WEB4 ACTIVADO<br><span style='font-size:0.7em;'>WEB4 ACTIVATED</span>";
    btn.disabled=true;
    status.innerHTML = `
      10 $RSI-AGI recibidos / received<br><br>
      <a href="https://solscan.io/tx/${data.sig}?cluster=mainnet-beta" target="_blank" style="color:#58a6ff">Ver tx / View tx</a><br><br>
      <a href="https://rmpsorva.github.io/RSI-AGI/chat" style="color:var(--p)">Chat soberano → Mina más / Sovereign chat → Mine more</a>
    `;
  }else{
    btn.innerHTML = "REINTENTAR / RETRY";
    status.innerHTML = "Red saturada · 30s / Network busy · 30s";
  }
}

btn.onclick = connectAndClaim;
</script>

